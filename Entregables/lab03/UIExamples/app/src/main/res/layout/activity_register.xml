<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background"
        tools:context=".ActivityJopApp">
    <EditText
            android:id="@+id/nameInput2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_person_svgrepo_com"
            android:ems="10"
            android:hint="Full Name"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="6dp"
            app:layout_constraintTop_toBottomOf="@+id/titleJob" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/name3"
            android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.809" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <EditText
            android:id="@+id/country2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_flag_country_svgrepo_com"
            android:ems="10"
            android:hint="Country"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/passr" android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/city2"/>
    <EditText
            android:id="@+id/emailid2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_email_svgrepo_com"
            android:ems="10"
            android:hint="E mail"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp"
            app:layout_constraintTop_toBottomOf="@+id/name3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/passr" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="19dp" android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"/>
    <EditText
            android:id="@+id/positionInput2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_work_ui_job_svgrepo_com"
            android:ems="10"
            android:hint="Applying for position"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="6dp"
            app:layout_constraintTop_toBottomOf="@+id/phoneNumber2" android:layout_marginBottom="6dp"
            app:layout_constraintBottom_toTopOf="@+id/register" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="19dp" android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:id="@+id/phoneNumber2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_phone_svgrepo_com"
            android:ems="10"
            android:hint="Phone Number"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/zipCodeInput2" android:layout_marginBottom="6dp"
            app:layout_constraintBottom_toTopOf="@+id/positionInput2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:id="@+id/zipCodeInput2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_mail_svgrepo_com"
            android:ems="10"
            android:hint="Zip Code"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/address2" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/phoneNumber2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="parent"/>

    <EditText
            android:id="@+id/name3"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_person_svgrepo_com__2_"
            android:ems="10"
            android:hint="User Name"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/nameInput2"
            app:layout_constraintBottom_toTopOf="@+id/emailid2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="19dp" android:layout_marginEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"/>
    <EditText
            android:id="@+id/address2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_address_svgrepo_com"
            android:ems="10"
            android:hint="Address"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/city2" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/zipCodeInput2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:id="@+id/city2"
            android:layout_width="367dp"
            android:layout_height="44dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_city_svgrepo_com"
            android:ems="10"
            android:hint="City"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/country2"
            app:layout_constraintBottom_toTopOf="@+id/address2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="17dp" android:layout_marginEnd="27dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Register"
            android:layout_width="115dp"
            android:layout_height="38dp" android:id="@+id/register"
            android:backgroundTint="@color/teal_200"
            android:background="@drawable/button_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="4dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="162dp" android:layout_marginEnd="161dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="6dp" app:layout_constraintTop_toBottomOf="@+id/positionInput2"/>
    <ImageView
            android:layout_width="105dp"
            android:layout_height="48dp"
            app:srcCompat="@drawable/ic_file_svgrepo_com"
            android:id="@+id/imageView"
            android:layout_marginEnd="175dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="133dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:layout_marginBottom="17dp"
            app:layout_constraintBottom_toTopOf="@+id/titleJob" tools:ignore="VectorDrawableCompat"
            android:tint="@color/black" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:id="@+id/passr"
            android:layout_width="372dp"
            android:layout_height="51dp"
            android:background="@drawable/edit_text_background"
            android:drawablePadding="10dp"
            android:drawableStart="@drawable/ic_password_svgrepo_com"
            android:ems="10"
            android:hint="Password"
            android:inputType="textPassword"
            android:padding="10dp"
            android:textColor="@color/white"
            android:textColorHint="@color/hint_color"
            android:textSize="21sp"
            app:layout_constraintTop_toBottomOf="@+id/emailid2" android:layout_marginTop="8dp"
            android:layout_marginEnd="23dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/country2"/>
    <TextView
            android:text="Register"
            android:layout_width="130dp"
            android:layout_height="29dp" android:id="@+id/titleJob"
            android:foregroundTint="@color/black"
            android:textSize="20sp"
            android:layout_marginEnd="123dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="99dp" android:textColor="@color/black"
            app:layout_constraintHorizontal_bias="1.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>