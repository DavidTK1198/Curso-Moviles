<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActivityJopApp" android:id="@+id/back">
    <TextView
            android:text="Entry Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/date"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
            android:layout_marginBottom="26dp" app:layout_constraintBottom_toTopOf="@+id/btnBack"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/nameInput"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="113dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="65dp"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toTopOf="@+id/lastNameInput"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Last Name"
            android:ems="10"
            android:id="@+id/lastNameInput"
            app:layout_constraintTop_toBottomOf="@+id/nameInput"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_marginEnd="18dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/address1Input"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Street Address"
            android:ems="10"
            android:id="@+id/address1Input"
            android:layout_marginTop="4dp" app:layout_constraintTop_toBottomOf="@+id/lastNameInput"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            android:layout_marginEnd="20dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/address2Input"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:text="Street Address 2"
            android:ems="10"
            android:id="@+id/address2Input"
            android:layout_marginBottom="9dp" app:layout_constraintBottom_toTopOf="@+id/stateInput"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="118dp"
            android:layout_marginTop="6dp" app:layout_constraintTop_toBottomOf="@+id/address1Input"
            android:layout_marginEnd="18dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="173dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="State/Province"
            android:ems="10"
            android:id="@+id/stateInput"
            android:layout_marginStart="2dp" app:layout_constraintStart_toEndOf="@+id/cityInput"
            android:layout_marginEnd="18dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/country"
            app:layout_constraintTop_toBottomOf="@+id/address2Input"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="City"
            android:ems="10"
            android:id="@+id/cityInput"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="118dp"
            android:layout_marginTop="2dp" app:layout_constraintTop_toBottomOf="@+id/address2Input"
            app:layout_constraintEnd_toStartOf="@+id/stateInput" android:layout_marginEnd="2dp"
            app:layout_constraintBottom_toTopOf="@+id/zipCodeInput" android:layout_marginBottom="4dp"/>
    <Button
            android:text="Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnBack"
            android:backgroundTint="@color/teal_200"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="161dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
            android:layout_marginEnd="162dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/Dateinput"/>
    <EditText
            android:layout_width="285dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:text="Position"
            android:ems="10"
            android:id="@+id/positionInput"
            android:layout_marginStart="15dp"
            app:layout_constraintStart_toEndOf="@+id/pos" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/phoneNumber"
            android:layout_marginBottom="5dp" app:layout_constraintBottom_toTopOf="@+id/Dateinput"/>
    <EditText
            android:layout_width="285dp"
            android:layout_height="43dp"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/Dateinput" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/btnBack" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/positionInput" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/date"/>
    <EditText
            android:layout_width="192dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:text="Phone Number"
            android:ems="10"
            android:id="@+id/phoneNumber"
            app:layout_constraintStart_toEndOf="@+id/areaCode" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="11dp"
            app:layout_constraintBottom_toTopOf="@+id/positionInput" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/emailid"/>
    <EditText
            android:layout_width="92dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:text="Area Code"
            android:ems="10"
            android:id="@+id/areaCode"
            android:layout_marginStart="7dp" app:layout_constraintStart_toEndOf="@+id/phone"
            app:layout_constraintEnd_toStartOf="@+id/phoneNumber" android:layout_marginBottom="11dp"
            app:layout_constraintBottom_toTopOf="@+id/positionInput" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/emailid"
            app:layout_constraintVertical_bias="0.9"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="40dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/emailid"
            android:layout_marginStart="62dp" app:layout_constraintStart_toEndOf="@+id/email"
            android:layout_marginEnd="18dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/phoneNumber" android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/country"/>
    <EditText
            android:layout_width="173dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Country"
            android:ems="10"
            android:id="@+id/country"
            android:layout_marginEnd="18dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="3dp"
            app:layout_constraintStart_toEndOf="@+id/zipCodeInput" android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toTopOf="@+id/emailid" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/stateInput"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Zip Code"
            android:ems="10"
            android:id="@+id/zipCodeInput"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="118dp"
            app:layout_constraintEnd_toStartOf="@+id/country" android:layout_marginEnd="3dp"
            android:layout_marginTop="3dp" app:layout_constraintTop_toBottomOf="@+id/cityInput"
            android:layout_marginBottom="9dp" app:layout_constraintBottom_toTopOf="@+id/emailid"/>
    <TextView
            android:text="Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="125dp" app:layout_constraintEnd_toStartOf="@+id/nameInput"
    />
    <TextView
            android:text="Last Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_marginBottom="36dp"
            app:layout_constraintBottom_toTopOf="@+id/textView6" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintEnd_toStartOf="@+id/lastNameInput"
            android:layout_marginEnd="32dp"/>
    <TextView
            android:text="Address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            android:layout_marginBottom="186dp"
            app:layout_constraintBottom_toTopOf="@+id/email"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintEnd_toStartOf="@+id/address1Input"
            android:layout_marginEnd="50dp"/>
    <TextView
            android:text="E mail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/email"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="36dp"
            android:layout_marginBottom="44dp" app:layout_constraintBottom_toTopOf="@+id/phone"
    />
    <TextView
            android:text="Phone Number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/phone"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginBottom="19dp" app:layout_constraintBottom_toTopOf="@+id/pos"/>
    <TextView
            android:text="Applying for position"
            android:layout_width="84dp"
            android:layout_height="34dp" android:id="@+id/pos"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginBottom="34dp" app:layout_constraintBottom_toTopOf="@+id/date"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <ImageView
            android:layout_width="104dp"
            android:layout_height="58dp" app:srcCompat="@drawable/_095027" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="48dp" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toTopOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="11dp" app:layout_constraintEnd_toStartOf="@+id/titleJob"
            android:layout_marginEnd="21dp"/>
    <TextView
            android:text="Job Application"
            android:layout_width="189dp"
            android:layout_height="26dp" android:id="@+id/titleJob"
            android:foregroundTint="@color/black" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="34dp" android:layout_marginEnd="30dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="1dp" app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginBottom="1dp" app:layout_constraintBottom_toTopOf="@+id/textView3"/>
    <TextView
            android:text="Please complete the form"
            android:layout_width="175dp"
            android:layout_height="23dp" android:id="@+id/textView3"
            android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/titleJob"
            android:layout_marginBottom="22dp"
            app:layout_constraintBottom_toTopOf="@+id/nameInput" android:layout_marginEnd="74dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="30dp"
            app:layout_constraintStart_toEndOf="@+id/imageView"/>
</androidx.constraintlayout.widget.ConstraintLayout>