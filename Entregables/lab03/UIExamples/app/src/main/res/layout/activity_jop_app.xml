<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActivityJopApp">
    <TextView
            android:text="Entry Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/date"
            app:layout_constraintEnd_toStartOf="@+id/Dateinput"
            android:layout_marginEnd="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="45dp" app:layout_constraintTop_toBottomOf="@+id/pos"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="81dp"
            app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:inputType="none"
            android:text="Name"
            android:ems="10"
            android:id="@+id/nameInput"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="88dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="65dp"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_marginBottom="15dp" app:layout_constraintBottom_toTopOf="@+id/cityInput"
            app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:text="Street Address"
            android:ems="10"
            android:inputType="none"
            android:id="@+id/address1Input"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            android:layout_marginEnd="20dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.93" android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/country" android:layout_marginBottom="21dp"
            app:layout_constraintBottom_toTopOf="@+id/zipCodeInput"/>
    <EditText
            android:layout_width="173dp"
            android:layout_height="46dp"
            android:text="State/Province"
            android:ems="10"
            android:inputType="none"
            android:id="@+id/stateInput"
            android:layout_marginStart="18dp"
            app:layout_constraintStart_toEndOf="@+id/textView5" android:layout_marginEnd="119dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/cityInput" android:layout_marginBottom="13dp"
            app:layout_constraintBottom_toTopOf="@+id/country" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="46dp"
            android:text="City"
            android:ems="10"
            android:inputType="none"
            android:id="@+id/cityInput"
            android:layout_marginEnd="180dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="60dp"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/nameInput" app:layout_constraintHorizontal_bias="0.833"/>
    <Button
            android:text="Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/register"
            android:backgroundTint="@color/teal_200"
            android:layout_marginTop="2dp"
            app:layout_constraintTop_toBottomOf="@+id/Dateinput" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="162dp" android:layout_marginEnd="161dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="285dp"
            android:layout_height="0dp"
            android:text="Position"
            android:ems="10"
            android:inputType="none"
            android:id="@+id/positionInput"
            android:layout_marginEnd="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toTopOf="@+id/Dateinput"
            android:layout_marginStart="12dp" app:layout_constraintStart_toEndOf="@+id/pos"
    />
    <EditText
            android:layout_width="285dp"
            android:layout_height="43dp"
            android:ems="10"
            android:inputType="none"
            android:id="@+id/Dateinput"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/date" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/positionInput"
            android:clickable="false" android:editable="false"/>
    <EditText
            android:layout_width="192dp"
            android:layout_height="43dp"
            android:inputType="none"
            android:text="Phone Number"
            android:ems="10"
            android:id="@+id/phoneNumber"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="30dp" app:layout_constraintStart_toEndOf="@+id/phone"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/positionInput"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="40dp"
            android:inputType="none|textEmailAddress"
            android:ems="10"
            android:id="@+id/emailid"
            android:layout_marginStart="27dp"
            app:layout_constraintStart_toEndOf="@+id/email" android:layout_marginEnd="46dp"
            app:layout_constraintEnd_toEndOf="parent" android:editable="false"
            app:layout_constraintTop_toBottomOf="@+id/zipCodeInput" android:layout_marginBottom="23dp"
            app:layout_constraintBottom_toTopOf="@+id/phoneNumber" app:layout_constraintHorizontal_bias="0.186"/>
    <EditText
            android:layout_width="173dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Country"
            android:ems="10"
            android:id="@+id/country"
            android:layout_marginEnd="112dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="63dp"
            app:layout_constraintStart_toEndOf="@+id/textView7" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="13dp" app:layout_constraintTop_toBottomOf="@+id/stateInput"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:text="Zip Code"
            android:ems="10"
            android:id="@+id/zipCodeInput"
            android:layout_marginEnd="128dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="51dp"
            app:layout_constraintStart_toEndOf="@+id/textView8" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/address1Input"
            app:layout_constraintBottom_toTopOf="@+id/emailid" android:layout_marginBottom="5dp"/>
    <TextView
            android:text="Full Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            tools:layout_editor_absoluteX="29dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="38dp"
            android:layout_marginBottom="23dp" app:layout_constraintBottom_toTopOf="@+id/textView8"/>
    <TextView
            android:text="E mail"
            android:layout_width="47dp"
            android:layout_height="29dp" android:id="@+id/email"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="52dp"
            android:layout_marginTop="41dp" app:layout_constraintTop_toBottomOf="@+id/textView8"
    />
    <TextView
            android:text="Phone Number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/phone"
            android:layout_marginTop="34dp" app:layout_constraintTop_toBottomOf="@+id/email"
            android:layout_marginBottom="31dp" app:layout_constraintBottom_toTopOf="@+id/pos"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
    />
    <TextView
            android:text="Applying for position"
            android:layout_width="84dp"
            android:layout_height="34dp" android:id="@+id/pos"
            android:layout_marginTop="31dp"
            app:layout_constraintTop_toBottomOf="@+id/phone" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp"
            app:layout_constraintEnd_toStartOf="@+id/positionInput" android:layout_marginEnd="12dp"/>
    <ImageView
            android:layout_width="104dp"
            android:layout_height="58dp" app:srcCompat="@drawable/_095027" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintEnd_toStartOf="@+id/titleJob"
            android:layout_marginEnd="1dp" app:layout_constraintStart_toStartOf="parent"
    />
    <TextView
            android:text="Job Application"
            android:layout_width="189dp"
            android:layout_height="26dp" android:id="@+id/titleJob"
            android:foregroundTint="@color/black" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" android:layout_marginEnd="30dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="1dp" app:layout_constraintStart_toEndOf="@+id/imageView"
            android:textSize="20sp" app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="38dp"
            app:layout_constraintBottom_toTopOf="@+id/nameInput"/>
    <TextView
            android:text="City"
            android:layout_width="58dp"
            android:layout_height="20dp" android:id="@+id/textView3"
            android:textSize="16sp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="24dp" app:layout_constraintBottom_toTopOf="@+id/textView5"
            app:layout_constraintVertical_bias="0.076"/>
    <TextView
            android:text="State/Province"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="22dp" app:layout_constraintEnd_toStartOf="@+id/stateInput"
            android:layout_marginEnd="18dp"
    />
    <TextView
            android:text="Country"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="36dp"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/textView5"
    />
    <TextView
            android:text="Zip Code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"
            android:layout_marginBottom="38dp" app:layout_constraintBottom_toTopOf="@+id/email"
            android:layout_marginTop="23dp" app:layout_constraintTop_toBottomOf="@+id/textView6"/>
</androidx.constraintlayout.widget.ConstraintLayout>